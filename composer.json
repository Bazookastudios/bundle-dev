{
  "name": "bazookastudios/bundle-dev",
  "license": "proprietary",
  "type": "project",
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Bazookas\\": "bundles/Bazookas/",
      "Nitro\\": "bundles/Nitro/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">7.2.0",
    "ext-json": "*",
    "league/flysystem-aws-s3-v3": "^1.0",
    "liip/imagine-bundle": "^2.2",
    "ocramius/proxy-manager": "^2.2",
    "oneup/flysystem-bundle": "^3.1",
    "php-ffmpeg/php-ffmpeg": "^0.14.0",
    "ramsey/uuid": "^3.8",
    "symfony/asset": "^4.3",
    "symfony/flex": "^1.4",
    "symfony/form": "^4.3",
    "symfony/lock": "^4.3",
    "symfony/monolog-bundle": "^3.4",
    "symfony/orm-pack": "^1.0",
    "symfony/proxy-manager-bridge": "^3.0",
    "symfony/security-bundle": "^4.3",
    "symfony/swiftmailer-bundle": "^3.3",
    "symfony/templating": "^4.3",
    "symfony/translation": "^4.3",
    "symfony/twig-pack": "^1.0",
    "symfony/validator": "^4.3",
    "symfony/web-link": "^4.3",
    "symfony/yaml": "^4.3",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "roave/security-advisories": "dev-master",
    "symfony/dotenv": "^4.3",
    "symfony/profiler-pack": "^1.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.ops.bazookas.be"
    }
  ],
  "scripts": {
    "post-install-cmd": []
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
